<C언어>
#include <stdio.h>

main() {
	
	int i,j,k,c = 0;
	int r,g,b;
	scanf("%d %d %d", &r, &g, &b);
	
	for(i = 0;i < r;i++) {
		for(j = 0;j < g;j++) {
			for(k = 0;k < b;k++) {
				printf("%d %d %d\n", i, j, k);\
				c++;
			}
		}
	}
	printf("%d", c);
	
}

<파이썬>
r,g,b = input().split()
for i in range(0,int(r)):
    for j in range(0,int(g)):
        for k in range(0,int(b)):
            print(i,j,k)
print(int(r)*int(g)*int(b))
